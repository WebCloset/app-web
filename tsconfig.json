{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["dom", "es2022"],
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": false,
    "noEmit": true,
    "resolveJsonModule": true,
    "incremental": true,
    "paths": { "@/*": ["./*"] },
    "plugins": [{ "name": "next" }]
  },
  "include": [".next/types/**/*.ts", "**/*.ts", "**/*.tsx"]
}
